{
  "expo": {
    "name": "Beebot Partner",
    "slug": "beebotpartner",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "beebot",
    "primaryColor": "#ffc107",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "backgroundColor": "#000000",
        "image": "./src/assets/splash_dark.png"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos for product installation proof."
        }
      ]
    ],
    "ios": {
      "buildNumber": "1.0.3",
      "supportsTablet": true,
      "bundleIdentifier": "asia.beebot.partner",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "NSLocationWhenInUseUsageDescription": "Your location is used to check closest store from you.",
        "NSPhotoLibraryUsageDescription": "Your chosen photo is being used for product installation proof.",
        "NSCameraUsageDescription": "Your camera is being used to take a picture of product installation and scan beebot's product qr code for warranty input purposes.",
      }
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "asia.beebot.partner",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d900ef5b-612c-4897-a86e-9fe3e9ea7473"
      }
    }
  }
}
